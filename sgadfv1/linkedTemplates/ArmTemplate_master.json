{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "sgadfv1"
		},
		"sg_db_ls_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'sg_db_ls'"
		},
		"sg_adls_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://singaporecomadls.dfs.core.windows.net/"
		},
		"sg_databricks_ls_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "1110-192349-8spb6kcr"
		},
		"sg_db_ls_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sgsqlserver1.database.windows.net"
		},
		"sg_db_ls_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sgsqldb1"
		},
		"sg_db_ls_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminmanas"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "sgadfv1_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"sg_db_ls_password": {
						"value": "[parameters('sg_db_ls_password')]"
					},
					"sg_adls_ls_properties_typeProperties_url": {
						"value": "[parameters('sg_adls_ls_properties_typeProperties_url')]"
					},
					"sg_databricks_ls_properties_typeProperties_existingClusterId": {
						"value": "[parameters('sg_databricks_ls_properties_typeProperties_existingClusterId')]"
					},
					"sg_db_ls_properties_typeProperties_server": {
						"value": "[parameters('sg_db_ls_properties_typeProperties_server')]"
					},
					"sg_db_ls_properties_typeProperties_database": {
						"value": "[parameters('sg_db_ls_properties_typeProperties_database')]"
					},
					"sg_db_ls_properties_typeProperties_userName": {
						"value": "[parameters('sg_db_ls_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}